* S/36 procedure to copy C subroutine members
* from build to ship libraries
REMOVE ALL,SUBR,#CEALIB
CONDENSE #CEALIB
LIBRLIBR LIBC,#CEALIB,SUBR,ALL
LIBRLIBR LIBA,#CEALIB,SUBR,ALL
LIBRLIBR LIBB,#CEALIB,SUBR,ALL
LIBRLIBR LIBM,#CEALIB,SUBR,ALL
REMOVE ALL,SUBR,#CALIB
CONDENSE #CALIB
LIBRLIBR LIBA,#CALIB,SUBR,ALL
LIBRLIBR LIBB,#CALIB,SUBR,ALL
LIBRLIBR LIBM,#CALIB,SUBR,ALL
REMOVE ALL,SUBR,#CPLIB
CONDENSE #CPLIB
LIBRLIBR LIBP,#CPLIB,SUBR,ALL
LIBRLIBR LIBC,#CPLIB,SUBR,ALL
LIBRLIBR LIBW,#CPLIB,SUBR,ALL
LIBRLIBR LIBB,#CPLIB,SUBR,ALL
LIBRLIBR LIBM,#CPLIB,SUBR,ALL
